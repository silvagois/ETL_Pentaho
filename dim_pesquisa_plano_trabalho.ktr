    <sql>/*
 * Cria sequence e tabela se não existir.
 */
DO
$$
BEGIN
	CREATE SEQUENCE data_warehouse.dim_pesquisa_plano_trabalho_dim_id_seq;
	CREATE TABLE data_warehouse.dim_pesquisa_plano_trabalho AS 
		SELECT nextval('data_warehouse.dim_pesquisa_plano_trabalho_dim_id_seq') AS dim_id, 
		1 AS version, 
		'1900-01-01 00:00:00.000000'::timestamp AS date_from, 
		'2199-12-31 23:59:59.999999'::timestamp AS date_to, 
		* FROM data_stage.pesquisa_plano_trabalho;
EXCEPTION WHEN duplicate_table THEN
        -- RAISE NOTICE 'do nothing, its already there';
END
$$ LANGUAGE plpgsql;

WITH 

/* Insere na dimensão alvo registros novos em relação ao step 2 */
step1 AS (
	INSERT INTO data_warehouse.dim_pesquisa_plano_trabalho
	SELECT nextval('data_warehouse.dim_pesquisa_plano_trabalho_dim_id_seq'), 1, '1900-01-01 00:00:00.000000'::timestamp, '2199-12-31 23:59:59.999999'::timestamp, pesquisa_plano_trabalho.*
	FROM data_stage.pesquisa_plano_trabalho
	WHERE NOT EXISTS (
		SELECT 1
		FROM data_warehouse.dim_pesquisa_plano_trabalho dim
		WHERE dim.nk_id_plano_trabalho = pesquisa_plano_trabalho.nk_id_plano_trabalho
	)
	RETURNING 1
),

/* Atualiza na dimensão alvo a coluna date_to dos registros com hash diferentes do step 2 */
step2 AS (
	UPDATE data_warehouse.dim_pesquisa_plano_trabalho
	SET date_to = current_timestamp
	FROM data_stage.pesquisa_plano_trabalho
	WHERE dim_pesquisa_plano_trabalho.date_to = '2199-12-31 23:59:59.999999'::timestamp
	AND pesquisa_plano_trabalho.nk_id_plano_trabalho = dim_pesquisa_plano_trabalho.nk_id_plano_trabalho
	AND pesquisa_plano_trabalho.row_hash &lt;&gt; dim_pesquisa_plano_trabalho.row_hash
	RETURNING dim_pesquisa_plano_trabalho.nk_id_plano_trabalho, dim_pesquisa_plano_trabalho.version
),

/* Insere na dimensão alvo nova versão dos registros com hash diferentes do step 7 */
step3 AS (
	INSERT INTO data_warehouse.dim_pesquisa_plano_trabalho
	SELECT nextval('data_warehouse.dim_pesquisa_plano_trabalho_dim_id_seq'), step2.version + 1, NOW(), '2199-12-31 23:59:59.999999'::timestamp, pesquisa_plano_trabalho.*
	FROM data_stage.pesquisa_plano_trabalho
	INNER JOIN step2 ON pesquisa_plano_trabalho.nk_id_plano_trabalho = step2.nk_id_plano_trabalho
	RETURNING 1
),

upsert AS (
     UPDATE data_warehouse.info_carga SET data_ultima_carga = NOW() 
     WHERE tabela_alvo = 'data_warehouse.dim_pesquisa_plano_trabalho' 
     RETURNING 1
),

executar_upsert AS (
	INSERT INTO data_warehouse.info_carga (tabela_alvo, data_ultima_carga) 
	SELECT 'data_warehouse.dim_pesquisa_plano_trabalho', NOW()
	WHERE NOT EXISTS (SELECT * FROM upsert)
	RETURNING 1
)

SELECT COUNT(*) FROM step1 AS inserts
UNION ALL
SELECT COUNT(*) FROM step3 AS updates
UNION ALL
SELECT * FROM executar_upsert

</sql>
